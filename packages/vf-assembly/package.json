{
  "name": "@vf/assembly",
  "version": "1.1.3",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.12.3"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "scripts": {
    "build": "asc ../../node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b build/main.wasm --optimize",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc ../../node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts --target release",
    "build:dev": "npm run asbuild:debug && npm run asbuild:release",
    "test": "node tests",
    "start": "npx serve ."
  },
  "dependencies": {
    "as-bind": "^0.3.4"
  }
}
