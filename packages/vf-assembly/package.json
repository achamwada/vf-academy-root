{
  "name": "@vf/assembly",
  "version": "1.1.3",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "0.19.1"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "scripts": {
    "build": "asc assembly/index.ts --exportRuntime --transform as-bind -b build/main.wasm --optimize",
    "asbuild": "asc ../../node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b build/main.wasm --optimize",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc ../../node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts --target release",
    "build:dev": "npm run asbuild:debug && npm run asbuild:release",
    "test": "node tests",
    "start": "npx serve ."
  },
  "dependencies": {
    "@assemblyscript/loader": "0.19.1",
    "as-bind": "^0.8.2",
    "assemblyscript": "0.19.1"
  }
}
